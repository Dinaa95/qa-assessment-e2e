{
  "name": "bymiles-quoteflow",
  "version": "0.1.0",
  "private": true,
  "homepage": "/v2",
  "scripts": {
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cy:run": "cypress run",
    "mochawesome": "marge cypress/reports/mochawesome/*.html -o cypress/reports/",
    "generate-mochawesome-report": "marge cypress/reports/mochawesome/*.html --reportDir cypress/reports/"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": false
  },
  "devDependencies": {
    "cypress": "^13.6.4",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "mochawesome": "^7.1.3"
  },
  "lint-staged": {
    "*.+(js|jsx)": [
      "eslint src --fix"
    ]
  }
}